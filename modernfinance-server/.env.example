# Server Configuration
NODE_ENV=production
PORT=8080
API_VERSION=v1

# CORS Configuration
CORS_ORIGINS=https://your-ios-app.com,http://localhost:3000

# Logging Configuration
LOG_LEVEL=info
LOG_FORMAT=json

# Redis Configuration (Railway provides these)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0
REDIS_TLS=false

# Security
JWT_SECRET=your-secure-jwt-secret-key-here-change-this-in-production
ALLOWED_ORIGINS=https://your-ios-app.com

# External Market Data APIs (Optional - for future use)
ALPHA_VANTAGE_API_KEY=
YAHOO_FINANCE_API_KEY=

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=60

# Cache Configuration
CACHE_TTL_SHORT=300
CACHE_TTL_DEFAULT=3600
CACHE_TTL_LONG=86400

# LLM APIs (Not needed for caching server, but kept for reference)
# OPENAI_API_KEY=
# ANTHROPIC_API_KEY=
# GOOGLE_API_KEY=

# LLM Model Configuration (optional - defaults shown)
# OPENAI_MODEL=gpt-4o
# ANTHROPIC_MODEL=claude-3-opus-20240229
# GOOGLE_MODEL=gemini-pro